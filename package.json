{
  "name": "raycast-mycolor",
  "displayName": "MyColor",
  "version": "1.0.0",
  "description": "Quick access to your favorite RGB colors",
  "icon": "color-palette.png",
  "author": "developer",
  "owner": "developer",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list-colors",
      "title": "List Colors",
      "description": "Browse and copy your favorite RGB colors",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^20.8.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18"
  }
}